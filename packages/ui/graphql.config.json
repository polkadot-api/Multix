{
  "overwrite": true,
  "schema": "https://polkadot.graphql.multix.live/graphql",
  "documents": "src/**/*.graphql",
  "generates": {
    "src/gql/": {
      "preset": "client",
      "plugins": []
    },
    "types-and-hooks.tsx": {
      "plugins": ["typescript", "typescript-operations", "typescript-react-query"],
      "config": {
        "reactQueryVersion": 5,
        "fetcher": {
          "func": "./src/hooks/useFetchData#useFetchData",
          "isReactHook": true
        }
      }
    }
  }
}
